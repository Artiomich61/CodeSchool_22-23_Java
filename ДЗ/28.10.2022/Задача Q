
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int a = scanner.nextInt(), b = scanner.nextInt();
        System.out.println(takeDays(a, b));
    }

    static int takeDays(int a, int b){
        return (b - 1 + a) / a;
    }

}
