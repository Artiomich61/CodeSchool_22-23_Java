import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        byte n = scanner.nextByte();
        printEndOfClass(n);
    }

    static void printEndOfClass(byte n){
        short mins = (short) (n * 45 + 5 * (n / 2) + 15 * (n - 1 - n / 2));
        System.out.println((9 + mins / 60) + " " + mins % 60);
    }

}
